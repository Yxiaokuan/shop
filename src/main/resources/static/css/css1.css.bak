}

.nowrap,ul.nowrapli li a {
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	display: inline-block;
	width: 90%;
	vertical-align: bottom;
	margin-left: -10px
}


ul.list-date li span.time {
	width: 80px;
	opacity: .7;
	font-family: arial;
	position: absolute;
	right: 0;
	top: 6px;
	text-align: right
}

